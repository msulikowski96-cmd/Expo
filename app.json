{
  "name": "CvToai",
  "slug": "cvtoai",
  "version": "1.0.0",
  "orientation": "portrait",
  "icon": "./assets/images/icon.png",
  "scheme": "cvtoai",
  "userInterfaceStyle": "automatic",
  "newArchEnabled": true,
  "ios": {
    "supportsTablet": true
  },
  "android": {
    "adaptiveIcon": {
      "foregroundImage": "./assets/images/adaptive-icon.png",
      "backgroundColor": "#ffffff"
    },
    "edgeToEdgeEnabled": true,
    "package": "com.yourname.cvtoai"  // Replace with your desired package name
  },
  "web": {
    "bundler": "metro",
    "output": "static",
    "favicon": "./assets/images/favicon.png"
  },
  "plugins": [
    "expo-router",
    [
      "expo-splash-screen",
      {
        "image": "./assets/images/splash-icon.png",
        "imageWidth": 200,
        "resizeMode": "contain",
        "backgroundColor": "#007bff"
      }
    ],
    [
      "expo-document-picker",
      {
        "iCloudContainerEnvironment": "Production"
      }
    ]
  ],
  "experiments": {
    "typedRoutes": true
  },
  "sdkVersion": "53.0.0",
  "platforms": [
    "ios",
    "android",
    "web"
  ],
  "extra": {
    "router": {},
    "eas": {
      "projectId": "84f2d6f1-8749-4d11-b186-df8dd590f29d"
    }
  },
  "androidStatusBar": {
    "backgroundColor": "#007bff"
  }
}
    "owner": "teabe79ths"
  }
}
